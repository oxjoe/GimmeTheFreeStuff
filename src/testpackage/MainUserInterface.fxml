    <Button fx:id="craigslistButton" layoutX="368.0" layoutY="23.0" mnemonicParsing="false" onAction="#craigslistClicked" prefHeight="25.0" prefWidth="129.0" text="Take me to Cragslist" AnchorPane.leftAnchor="368.0" AnchorPane.topAnchor="23.0" />
