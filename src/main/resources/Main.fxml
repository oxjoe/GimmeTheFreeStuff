<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            stylesheets="@main.css" xmlns="http://javafx.com/javafx/8.0.111"
            fx:controller="MainController">
  <children>
    <Button AnchorPane.leftAnchor="359.0" AnchorPane.topAnchor="16.0" layoutX="359.0" layoutY="16.0"
            mnemonicParsing="false" onAction="#craigslistClicked" prefHeight="38.0"
            prefWidth="129.0" text="Take me to Craigslist" fx:id="craigslistButton"/>
    <Button AnchorPane.leftAnchor="511.0" AnchorPane.topAnchor="16.0" layoutX="511.0" layoutY="16.0"
            mnemonicParsing="false" onAction="#gotoSettings" prefHeight="38.0" prefWidth="75.0"
            text="Settings" fx:id="settingsButton"/>
    <Button AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="16.0" layoutX="14.0" layoutY="16.0"
            mnemonicParsing="false" onAction="#updateListClicked" prefHeight="38.0" prefWidth="98.0"
            text="Update List" fx:id="updateListButton"/>
    <Text AnchorPane.topAnchor="57.734375" fill="#0abf0dcc" layoutX="87.0" layoutY="75.0" strokeType="OUTSIDE"
          strokeWidth="0.0" text="Refreshed List!" textAlignment="CENTER" visible="false"
          wrappingWidth="114.0546875" fx:id="successText">
      <font>
        <Font size="16.0"/>
      </font>
    </Text>
    <TableView AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="81.0"
               layoutY="81.0" prefHeight="319.0"
               prefWidth="600.0" fx:id="tableView">
      <columns>
        <TableColumn prefWidth="75.0" text="Status" fx:id="statusCol"/>
        <TableColumn prefWidth="146.0" text="Date Posted" fx:id="dateCol"/>
        <TableColumn prefWidth="259.0" text="Name" fx:id="nameCol"/>
        <TableColumn prefWidth="104.0" text="Websitelink" fx:id="urlCol"/>
      </columns>
    </TableView>
    <HBox layoutX="135.0" layoutY="35.0" prefHeight="21.0" prefWidth="200.0">
      <children>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Time Till Update:"
              wrappingWidth="98.05467987060547"/>
        <Label prefHeight="17.0" prefWidth="95.0" text="N/A" fx:id="countdownLabel"/>
      </children>
    </HBox>
    <HBox layoutX="112.0" layoutY="13.0" prefHeight="44.0" prefWidth="246.0" visible="false">
      <children>
        <Label prefHeight="38.0" prefWidth="28.0" text="Find:"/>
        <TextField onKeyPressed="#searchTextfieldEnter" prefHeight="38.0" prefWidth="204.0"
                   promptText="To be added" fx:id="searchTextfield"/>
      </children>
    </HBox>
  </children>
</AnchorPane>
